{
  "name": "@daotl/nodejs-effect-starter",
  "version": "0.0.1",
  "description": "Node.js + Effect starter template for DAOT projects",
  "private": true,
  "type": "module",
  "main": "./dist/main.js",
  "pnpm": {
    "patchedDependencies": {
      "@effect/language-service@0.0.19": "patches/@effect__language-service@0.0.19.patch",
      "@typescript-eslint/scope-manager@5.57.0": "patches/@typescript-eslint__scope-manager@5.57.0.patch",
      "eslint-plugin-codegen@0.17.0": "patches/eslint-plugin-codegen@0.17.0.patch",
      "typescript@5.0.0-tsplus.20230224": "patches/typescript@5.0.0-tsplus.20230224.patch",
      "date-fns@2.29.3": "patches/date-fns@2.29.3.patch",
      "@fp-ts/optic@0.6.0": "patches/@fp-ts__optic@0.6.0.patch"
    }
  },
  "scripts": {
    "circular": "pnpm circular:src && pnpm circular:dist",
    "circular:src": "madge --circular --ts-config ./tsconfig.json --extensions ts ./_src",
    "circular:dist": "madge --circular --extensions js ./dist",
    "clean": "rm -rf dist _test/dist",
    "clean:dist": "sh scripts/clean-dist.sh",
    "build": "pnpm clean:dist && NODE_OPTIONS=--max-old-space-size=6144 tsc --build",
    "rebuild": "pnpm clean && pnpm build",
    "build:watch": "pnpm build --watch",
    "build:watch2": "pnpm clean:dist && NODE_OPTIONS=--max-old-space-size=6144 tsc -w",
    "build:libs": "cd libs && pnpm build && cd ../.. && pnpm build",
    "compile": "NODE_OPTIONS=--max-old-space-size=6144 tsc --noEmit",
    "lint": "rome format . && rome check .",
    "lint:fix": "rome format --write . && rome check --apply .",
    "eslint": "NODE_OPTIONS=--max-old-space-size=6144 ESLINT_TS=1 eslint _src/** _test/**",
    "eslint:watch": "ESLINT_TS=1 esw -w --changed --clear --ext ts,tsx _src/** _test/**",
    "test": "vitest",
    "test:run": "pnpm run test run",
    "testsuite": "pnpm circular && pnpm run test:run && pnpm lint",
    "dev": "ts-node-esm -P tsconfig.src.json _src/main.ts",
    "dev:compiled": "pnpm start:compiled | scripts/humanlog.sh",
    "dev:compiled:watch": "NODE_ENV=development nodemon --exec pnpm dev:compiled",
    "start": "pnpm start:compiled",
    "start:compiled": "node --experimental-specifier-resolution=node -r source-map-support/register ./dist/main.js",
    "up-all": "pnpm --recursive update",
    "up-ncu": "pnpm run update && pnpm i",
    "update": "ncu -u && pnpm -r ncu -u",
    "ncu": "ncu",
    "nnm": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' + && pnpm i",
    "preinstall": "npx only-allow pnpm"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./_cjs/index.cjs"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      },
      "require": {
        "types": "./dist/*.d.ts",
        "default": "./_cjs/*.cjs"
      }
    }
  },
  "resolutions": {
    "@effect/data": "^0.7.4",
    "@effect/io": "^0.15.2",
    "@types/node": "~18.11.11",
    "date-fns": "^2.29.3",
    "http-proxy": "npm:http-proxy-node16@1.0.0"
  },
  "dependencies": {
    "@effect-app/infra": "^0.44.5",
    "@effect-app/prelude": "^0.33.4",
    "@effect-app/core": "^0.38.1",
    "@effect-app/infra-adapters": "^0.32.4",
    "@effect-app/schema": "^0.30.2",
    "@effect/io": "^0.15.2",
    "@effect/data": "^0.7.4",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@daotl/effect-app-eslint-codegen-model": "^0.11.0",
    "@daotl/rome-config": "0.0.1-alpha.4",
    "@effect-app/cli": "^0.9.0",
    "@effect-app/prelude": "^0.33.4",
    "@effect-app/core": "^0.38.1",
    "@effect-app/compiler": "^0.19.0",
    "@effect/language-service": "^0.0.19",
    "@tsconfig/node18-strictest-esm": "^1.0.1",
    "@tsplus/installer": "0.0.171",
    "@types/lodash": "^4.14.192",
    "@types/node": "~18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@typescript-eslint/scope-manager": "^5.57.0",
    "babel-plugin-replace-import-extension": "^1.1.3",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "enhanced-resolve": "^5.12.0",
    "eslint": "^8.37.0",
    "eslint-plugin-codegen": "^0.17.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-watch": "^8.0.0",
    "madge": "^6.0.0",
    "nodemon": "^2.0.22",
    "npm-check-updates": "^16.9.0",
    "prebuild-install": "^7.1.1",
    "rome": "^12.0.0",
    "ts-node": "^10.9.1",
    "ts-transform-paths": "^3.0.0",
    "tsc-watch": "^6.0.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "https://cdn.jsdelivr.net/npm/@tsplus/installer@0.0.171/compiler/typescript.tgz",
    "typescript-transform-paths": "^3.4.6",
    "unplugin-auto-import": "^0.15.2",
    "vitest": "^0.29.7"
  }
}