{
  "name": "@effect-app-boilerplate/e2e",
  "version": "1.0.0",
  "license": "NONE",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "playwright test",
    "test:watch": "playwright-watch test",
    "build": "tsc --build",
    "lint": "NODE_OPTIONS=--max-old-space-size=8192 ESLINT_TS=1 eslint .",
    "lint:watch": "ESLINT_TS=1 esw -w --changed --clear --ext ts,tsx .",
    "autofix": "pnpm lint --fix",
    "up": "pnpm run update && pnpm exec-update",
    "update": "pnpm ncu -u",
    "exec-update": "pnpm i",
    "clean": "rm -rf test-out",
    "ncu": "ncu",
    "watch": "pnpm build --watch"
  },
  "devDependencies": {
    "@playwright/test": "~1.53.1",
    "@types/node": "~24.0.3",
    "date-fns": "^4.1.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-import-resolver-webpack": "^0.13.10",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "npm-check-updates": "^18.0.1",
    "playwright-core": "^1.53.1",
    "playwright-watch": "^1.3.23",
    "prettier": "^3.5.3",
    "typescript": "~5.8.3"
  },
  "dependencies": {
    "@effect-app-boilerplate/api": "workspace:*",
    "@effect/platform-node": "0.86.4",
    "effect-app": "^2.41.0",
    "@effect/platform": "^0.85.2",
    "effect": "^3.16.8",
    "cross-fetch": "^4.1.0"
  }
}