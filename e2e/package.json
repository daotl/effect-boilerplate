{
  "name": "@macs-configurator/e2e",
  "version": "1.0.0",
  "license": "NONE",
  "private": true,
  "type": "module",
  "scripts": {
    "pretest": "pnpm build",
    "test": "playwright test -c test-out",
    "test:watch": "playwright-watch test",
    "build": "tsc --build",
    "eslint": "NODE_OPTIONS=--max-old-space-size=6144 ESLINT_TS=1 eslint .",
    "eslint:watch": "ESLINT_TS=1 esw -w --changed --clear --ext ts,tsx .",
    "autofix": "pnpm lint --fix",
    "up": "pnpm run update && pnpm exec-update",
    "update": "pnpm ncu -u",
    "exec-update": "pnpm i",
    "clean": "rm -rf test-out",
    "ncu": "ncu",
    "watch": "pnpm build --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/node": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-modules-commonjs": "^7.21.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@playwright/test": "~1.32.1",
    "@types/node": "~18.15.11",
    "babel-loader": "^9.1.2",
    "date-fns": "^2.29.3",
    "eslint": "^8.37.0",
    "npm-check-updates": "^16.9.0",
    "playwright-core": "^1.32.1",
    "playwright-watch": "^1.3.23",
    "ts-loader": "^9.4.2",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "https://cdn.jsdelivr.net/npm/@tsplus/installer@0.0.171/compiler/typescript.tgz",
    "webpack": "^5.77.0"
  },
  "dependencies": {
    "@effect-app-boilerplate/resources": "*",
    "@effect-app-boilerplate/models": "*",
    "@effect-app/prelude": "^0.33.4",
    "@effect-app/core": "^0.38.1",
    "@effect-app/schema": "^0.30.2",
    "@effect-app/vue": "^0.31.6",
    "@effect/io": "^0.15.2",
    "@effect/data": "^0.7.4",
    "@tsplus/stdlib": "^0.2.8",
    "cross-fetch": "^3.1.5",
    "tsconfig-paths": "^4.2.0"
  }
}